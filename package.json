{
  "name": "@cdk-lambda-layers/node-aws-sdk",
  "description": "AWS CDK Lambda Layer for Node.js with newer version of aws-sdk package",
  "version": "2.496.0-alpha.0",
  "keywords": [
    "aws",
    "cdk",
    "lambda",
    "layer",
    "node",
    "nodejs",
    "aws-sdk",
    "cloud development kit",
    "lambda layer version"
  ],
  "author": "Tamino Martinius <dev@zaku.eu>",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tamino-martinius/cdk-lambda-layers--node-aws-sdk.git"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "prebuild": "rm -rf dist/*",
    "build": "tsc",
    "watch": "tsc -w",
    "prepack": "cd nodejs && npm i --production",
    "pack": "rm dist/nodejs.zip; zip dist/nodejs.zip nodejs -r",
    "preversion": "npm run build && npm run pack"
  },
  "devDependencies": {
    "@types/node": "12.6.8",
    "aws-cdk": "^1.1.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda": "^1.1.0",
    "@aws-cdk/core": "^1.1.0"
  }
}
